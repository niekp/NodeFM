extends ../masterpage/layout-list

append before-table
	if datefilter
		// recent-tracks::before-table
		include ../shared/date-filter

append table-content
	// recent-tracks::table-content
	
	thead
		tr
			th Artist
			th Track
			th 
	tbody
		each val in tracks
			tr(data-type='track')
				td(data-search='artist') #{val.artist}
				td(data-search='track') #{val.track}
				td #{moment(val.utc, 'X').format('L LT')}